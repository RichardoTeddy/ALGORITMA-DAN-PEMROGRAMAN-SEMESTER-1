import java.util.Scanner;

public class MenentukanSegitiga {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Masukan Panjang Sisi A        : ");
        int intA = input.nextInt();
        System.out.print("Masukan Panjang Sisi B        : ");
        int intB = input.nextInt();
        System.out.print("Masukan Panjang Sisi C        : ");
        int intC = input.nextInt();

        System.out.println();

        if ((intC * intC) == (intB * intB) + (intA * intA)) {
            if ((intA == intB) || (intA == intC)) {
                System.out.println("Segitiga dengan Panjang Sisi A :" + intA + ", Sisi B : " + intB + ", Sisi C : " + intC + " termasuk Segitiga Siku Siku Sama Kaki");
            } else {
                System.out.println("Segitiga dengan Panjang Sisi A :" + intA + ", Sisi B : " + intB + ", Sisi C : " + intC + " termasuk Segitiga Siku Siku");
            }
        } else if ((intB * intB) == (intC * intC) + (intA * intA)) {
            if ((intA == intB) || (intA == intC)) {
                System.out.println("Segitiga dengan Panjang Sisi A :" + intA + ", Sisi B : " + intB + ", Sisi C : " + intC + " termasuk Segitiga Siku Siku Sama Kaki");
            } else {
                System.out.println("Segitiga dengan Panjang Sisi A :" + intA + ", Sisi B : " + intB + ", Sisi C : " + intC + " termasuk Segitiga Siku Siku");
            }
        } else if ((intA * intA) == (intC * intC) + (intB * intB)) {
            if ((intA == intB) || (intA == intC)) {
                System.out.println("Segitiga dengan Panjang Sisi A :" + intA + ", Sisi B : " + intB + ", Sisi C : " + intC + " termasuk Segitiga Siku Siku Sama Kaki");
            } else {
                System.out.println("Segitiga dengan Panjang Sisi A :" + intA + ", Sisi B : " + intB + ", Sisi C : " + intC + " termasuk Segitiga Siku Siku");
            }
        } else if ((intA == intB) && (intB == intC)) {
            System.out.println("Segitiga dengan Panjang Sisi A :" + intA + ", Sisi B : " + intB + ", Sisi C : " + intC + " termasuk Segitiga Sama Sisi");
        } else if ((intA == intB) || (intA == intC)) {
            System.out.println("Segitiga dengan Panjang Sisi A :" + intA + ", Sisi B : " + intB + ", Sisi C : " + intC + " termasuk Segitiga Sama Kaki");
        } else {
            System.out.println("Segitiga dengan Panjang Sisi A :" + intA + ", Sisi B : " + intB + ", Sisi C : " + intC + " termasuk Segitiga Sembarang");
        }
        System.out.println("Program ini dibuat oleh Richardo Teddy_215314015");
